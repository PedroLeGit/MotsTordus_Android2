        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".vue.moncompte"></activity>
        <activity android:name=".vue.erreurinternet" />
        <activity android:name=".vue.concours" />
        <activity android:name=".vue.questions" />
        <activity android:name=".vue.mentions_legales" />
        <activity android:name=".vue.Connexion">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".vue.MainActivity" />
    </application>

</manifest>